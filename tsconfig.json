{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "node",
    "declaration": false,
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "incremental": true
  },
  "include": ["src/**/*"]
}
